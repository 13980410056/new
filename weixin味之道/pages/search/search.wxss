/* pages/search/search.wxss */
.Carousel{
  height: 300rpx;
  width: 100%;
}
.Carousel-swiper{
  height: 300rpx;
}
.Carousel-image{
  width: 100%;
  height: 100%;
}
.search{
  width: 94%;
  height: 50rpx;
  border-radius:42px;
  margin: 10px auto 0;
  justify-content: center;
  border:2px solid #C1C1C1;
}
.search>input{
  width:80%;
  padding:0 0 0 15px;
  font-size: 26rpx;
  float: left
}
.search>view{
  width:10%;
  float:right;
  padding:1px 2px;
}
.search>view::before{
  padding:2px 10px;
  font-size:20px;
}



.lists{
  width: 45%;
  border-radius: 5px;
  overflow: hidden;
  height: 226rpx;
  border: 1px solid #ECECEC;
  margin:10px 2%; 
  float: left
}
.lists>image{
  width: 100%;
  height: 175rpx;
 
}
.lists>text{
  font-size: 24rpx;
  padding:0 0 0 5px;
  float:left;
}
.lists-txt1{
  width: 60%;
  height: 32rpx;
  overflow: hidden;
}
.lists-txt2{
  width: 30%;
  
}
.lists-txt2::before{
  display: inline-block;
  padding: 0 3px 0 10px ;
}


